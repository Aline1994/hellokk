package hellokk.thread.start;

public class ThreadBuildOneTest{
	public static void main(String[] args) {
		System.out.println(Thread.currentThread().getName());
		ThreadBuildOne tb = new ThreadBuildOne("zx");
		ThreadBuildOne tb1 = new ThreadBuildOne("kk");

		tb.start();
		tb1.start();
	}
}
class ThreadBuildOne extends Thread {
	private String name;
	public ThreadBuildOne(String name){
		this.name = name;
	}
	public void run(){
		for(int i=0;i<100;i++){
			System.out.println(this.name+" "+i);
		}
	}
}

