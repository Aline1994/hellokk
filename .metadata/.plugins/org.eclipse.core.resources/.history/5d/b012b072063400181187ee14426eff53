package com.jdk.thread.mianshi;

public class Jiaoti  {
	static class Printer implements Runnable{
		static int num;
		static final int END = 75;
		int id;
		public Printer(int id){
			this.id=id;
		}
		@Override
		public void run() {
			synchronized(Printer.class){
				while(num<END){
					if(num/5%3==id){
						System.out.println(id+":");
						for(int i=0;i<5;i++){
							System.out.print(num+++",");
						}
						System.out.println();
					}
				}
			}
		}
		
	}
}
