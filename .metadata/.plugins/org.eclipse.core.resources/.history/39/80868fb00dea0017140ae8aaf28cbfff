package hellokk.designDemo.proxy;

public class Proxy {
	private Source source;
	public Proxy(){
		super();
		this.source = source;
	}
	public void method(){
		before();
		source.method();
		after();
	}
	private void after() {
		System.out.println("proxy before");		
	}
	private void before() {
		System.out.println("proxy after");
	}
}
