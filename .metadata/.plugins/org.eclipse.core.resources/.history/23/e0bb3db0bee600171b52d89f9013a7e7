package test;
//break:结束内层的for循环
//return结束整个for循环，外层循环
//continue
public class BreakTest {
	public static void main(String[] args) {
		for(int i=0;i<10;i++){
			for(int j=0;j<i;j++){
				if(j==3){
					System.out.println(j);
					continue;
				}
			}
			System.out.println("******i*****"+i);
		}
	}
}
