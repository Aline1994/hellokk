package hellokk;

public class Bubble {
	public static void main(String[] args) {
		int [] x = {5,9,8,6,2};
		int [] y = sort(x);
		for(int z:y){
			System.out.println(z);
		}
	}
	//冒泡
	public static  int[] sort(int []x){
		int temp = 0;
		for(int i=0;i<x.length-1;i++){
			for(int j=0;j<x.length-i-1;j++){
				if(x[j]>x[j+1]){
					temp = x[j];
					x[j]=x[j+1];
					x[j+1]=temp;
				}
			}
		}
		return x;
	}
	
	//插入
	public static int[] insertsort(int[]x){
		for(int i=0;i<x.length;i++){
			
		}
	}
}
