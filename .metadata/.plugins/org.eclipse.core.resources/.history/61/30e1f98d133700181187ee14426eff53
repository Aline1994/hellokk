package com.jdk.thread.mianshi;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class ZhengFuJiaoti {
	public static void main(String[] args) {
		int a [] = {1,2,-3,-5,3,-1,-8,7};
		Arrays.sort(a);
		
		System.out.println();
		int temp = a[4];
		a[4]=a[1];
		a[1]=temp;
		for(int as:a){
			System.out.print(as+",");
		}
		System.out.println();
	}
}
